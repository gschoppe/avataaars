{
  "name": "@gschoppe/avataaars",
  "version": "2.2.2",
  "description": "React component for avataaars, updated with bugfixes, support for react 19, and new options.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/animation.css",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-styles": "copyfiles -u 1 src/*.css dist/",
    "build": "npm run clean && tsc && npm run copy-styles"
  },
  "keywords": [
    "react",
    "avatar"
  ],
  "repository": "git@github.com:gschoppe/avataaars.git",
  "author": "Greg Schoppe <contact@gschoppe.com> (https://gschoppe.com)",
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "copyfiles": "^2.4.1",
    "import-sort-style-renke": "^6.0.0",
    "rimraf": "^6.0.1",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "tslint-react": "^5.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "lodash.uniqueid": "^4.0.1",
    "prop-types": "^15.8.1"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "import-sort-style-renke"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "import-sort-style-renke"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": true
  }
}